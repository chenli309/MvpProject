<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_cw" />
            <stroke android:width="@dimen/x1" android:color="@color/color_cr1" />
            <corners android:radius="@dimen/x2" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_cbcccccc" />
            <stroke android:width="@dimen/x1" android:color="@color/color_cbcccccc" />
            <corners android:radius="@dimen/x2" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_cw" />
            <stroke android:width="@dimen/x1" android:color="@color/color_cr1" />
            <corners android:radius="@dimen/x2" />
        </shape>
    </item>
</selector>